{
    "name": "engraving/skeleton",
    "description": "Engraving HTTP Application Skeleton",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Maik Hilker",
            "email": "maikhilker89@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "php": "7.4.*",
        "ext-zend-opcache": "*",
        "engraving/engraving": "dev-master",
        "guzzlehttp/psr7": "^1.4",
        "http-interop/response-sender": "^1.0",
        "league/plates": "^3.3",
        "nikic/fast-route": "^1.3",
        "php-di/php-di": "^6.0"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Engraving\\App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Engraving\\App\\": "tests/"
        }
    }
}
